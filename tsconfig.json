{
  "compilerOptions": {
    "module": "ES2022",
    "moduleResolution": "node",
    "target": "ES2022",
    "resolveJsonModule": true,
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "useDefineForClassFields": false,
    "noEmit": true,
    "allowJs": true,
    "noImplicitAny": false,
    "outDir": "dist",
  },
  "include":["**/*.ts"],
}